@mixin fulldesctop {
  @media screen and (max-width: 1800px) {
    @content;
  }
}
@mixin desctop {
  @media screen and (max-width: 1200px) {
    @content;
  }
}
@mixin tablets {
  @media screen and (max-width: 768px) {
    @content;
  }
}
@mixin phones {
  @media screen and (max-width: 480px) {
    @content;
  }
}